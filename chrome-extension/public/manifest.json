{
  "name": "GraphQL state",
  "description": "Data visualization for GraphQL state",
  "version": "0.3.1",
  "manifest_version": 3,
  "devtools_page": "devtools.html",
  "icons": {
    "16": "images/16_16.png",
    "32": "images/32_32.png",
    "48": "images/48_48.png",
    "128": "images/128_128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content-script.js"]
    }
  ]
}